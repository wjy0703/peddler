        </property>        
    </bean>  
    
    <!-- 逾期报表统计 -->
    <bean id="yqbbExcelServiceConfiguration" class="cn.com.cucsi.app.excel.bean.ExcelModelConfiguration">
        <property name="title">
           <value>逾期报表</value>
        </property>
        <property name="reportMetaStr">
           <value>合同编号,机构,客户姓名,放款日期,客户经理,合同金额,期数,逾期日期,逾期天数,剩余期数,剩余本金,利率,应还期供,罚息利率,罚息,违约金利率,违约金,当前状态,已还金额,挂账金额</value>
        </property>
        <property name="reportMetalColumnStr">
           <value>JKHTBM,YYB,JKRXM,FKDATE,SALENAME,HTJE,HKQS,OVERDATE,OVERDAY,OVERQS,SYJE,DKLL,YHKJE,INTEREST,PUNISH_INTEREST,DAMAGES,DAMAGES_MONEY,OVERDUE_STATR,SPARE_FIELD06,SPARE_FIELD03</value>
        </property>
        <property name="reportColumnsWidthStr">
           <value>20,20,20,20,20,20,20,20,20,20,20,20,20,20,20,20,20,20,20,20</value>
        </property>      
    </bean>
    
    <!-- 汇金还款统计表 -->
    <bean id="loanBackExcelServiceConfiguration" class="cn.com.cucsi.app.excel.bean.ExcelModelConfiguration">
        <property name="title">
           <value>汇金还款统计表</value>
        </property>
        <property name="reportMetaStr">
           <value>[序号],基本信息[合同编号,客户名称,当前状态,合同金额,期数,机构,产品类型],还款计划[放款日期,还款日期,应还款总额,应还本金,应还利息,贷款余额,剩余还款期限],实际还款[实际还款日期,实还总额,实还本金,实还利息,贷款余额],[未付利息],客户存款[存款余额],[提前还款金额],[逾期借款金]</value>
        </property>
        <property name="reportMetalColumnStr">
           <value>ROWNUM,JKHTBM,CUSTOMERNAME,ACCOUNTSTATE,HTJE,HKQS,ORGNAZATIONNAME,LOANTYPE,MAKELOANDATE,NEXTDATEREAL,YHKZE,SJHKBJ,YHKLX,SYJE,SYHKYS,REALTIME,REALMONEY,SJHKBJ,SJHKLX,SYJE,REALDKLL,ACCOUNT_BALANCE,TQJQ,YQJKZE</value>
        </property>
        <property name="reportColumnsWidthStr">
           <value>20,20,20,20,20,20,20,20,20,20,20,20,20,20,20,20,20,20,20,20,20,20,20,20</value>
        </property>        
    </bean>  
</beans>