<?xml version="1.0" encoding="UTF-8"?>
<sqlmaps>
	<!-- -->
	<sql name="queryXhCreditTaskAssignList" common="true">
		<sqlString><![CDATA[
			select 
				a.LOAN_APPLY_ID,/*借款申请ID*/
				a.FIRST_AUDIT_EMPLOYEE_ID,/*初审人员ID*/
				a.SECOND_AUDIT_EMPLOYEE_ID,
			a.ID
			 from XH_CREDIT_TASK_ASSIGN a
			      where 1=1
			$sql
			]]></sqlString>
	</sql>
	<!-- 查询 总数 -->
	<sql name="cnt_queryXhCreditTaskAssignList" common="false">
		<sqlString>
			<![CDATA[
			SELECT COUNT(*) RecordCount FROM (
			$ibpm_queryXhCreditTaskAssignList
			)a
		]]>
		</sqlString>
	</sql>
	<!-- 查询 列表分页 -->
	<sql name="page_queryXhCreditTaskAssignList" common="false">
		<sqlString>
			<![CDATA[
			$ibpm_queryPageBefore
			$ibpm_queryXhCreditTaskAssignList
			$ibpm_queryPageAfter
			]]>
		</sqlString>
	</sql>
</sqlmaps>
