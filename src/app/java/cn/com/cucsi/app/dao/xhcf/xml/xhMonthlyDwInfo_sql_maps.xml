<?xml version="1.0" encoding="UTF-8"?>
<sqlmaps>
    <!--  -->
	 <sql name="queryXhMonthlyDwInfoList" common="true">
		<sqlString><![CDATA[
			select 
				a.ID,/*主键*/
				a.INTEREST,/*利息*/
				a.LOAN_NAME,/*借款人名称*/
				a.MONEY,/*金额*/
				a.ZQTJ_ID,/*月应付主表ID*/
			a.ID
			 from XH_MONTHLY_DW_INFO a
			      where 1=1
			$sql
			]]></sqlString>
	</sql>
	<!-- 查询 总数 -->
	<sql name="cnt_queryXhMonthlyDwInfoList" common="false">
		<sqlString>
			<![CDATA[
			SELECT COUNT(*) RecordCount FROM (
			$ibpm_queryXhMonthlyDwInfoList
			)a
		]]>
		</sqlString>
	</sql>
	<!-- 查询 列表分页 -->
	<sql name="page_queryXhMonthlyDwInfoList" common="false">
		<sqlString>
			<![CDATA[
			$ibpm_queryPageBefore
			$ibpm_queryXhMonthlyDwInfoList
			$ibpm_queryPageAfter
			]]>
		</sqlString>
	</sql>
</sqlmaps>
