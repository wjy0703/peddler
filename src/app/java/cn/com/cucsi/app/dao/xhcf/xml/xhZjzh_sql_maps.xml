<?xml version="1.0" encoding="UTF-8"?>
<sqlmaps>
    <!--  -->
	 <sql name="queryXhZjzhList" common="true">
		<sqlString><![CDATA[
			select 
				a.MIDDLE_MAN_NAME,/*中间人*/
				a.ADDR,/*地址*/
				a.ID_CARD,/*身份证号*/
				a.CRED_ADDR,/*开户行*/
				a.CRED_ID,/*卡号*/
			a.ID
			 from BASE_MIDDLE_MAN a
			      where 1=1
			$sql
			]]></sqlString>
	</sql>
	<!-- 查询 总数 -->
	<sql name="cnt_queryXhZjzhList" common="false">
		<sqlString>
			<![CDATA[
			SELECT COUNT(*) RecordCount FROM (
			$ibpm_queryXhZjzhList
			)a
		]]>
		</sqlString>
	</sql>
	<!-- 查询 列表分页 -->
	<sql name="page_queryXhZjzhList" common="false">
		<sqlString>
			<![CDATA[
			$ibpm_queryPageBefore
			$ibpm_queryXhZjzhList
			$ibpm_queryPageAfter
			]]>
		</sqlString>
	</sql>
</sqlmaps>
