<?xml version="1.0" encoding="UTF-8"?>
<sqlmaps>
    <!--  -->
	 <sql name="queryXhBlackListList" common="true">
		<sqlString><![CDATA[
			select 
				a.NAME,/*客户姓名*/
				a.IDENTIF_ID,/*客户身份证号*/
			a.ID
			 from XH_BLACK_LIST a
			      where 1=1
			$sql
			]]></sqlString>
	</sql>
	<!-- 查询 总数 -->
	<sql name="cnt_queryXhBlackListList" common="false">
		<sqlString>
			<![CDATA[
			SELECT COUNT(*) RecordCount FROM (
			$ibpm_queryXhBlackListList
			)a
		]]>
		</sqlString>
	</sql>
	<!-- 查询 列表分页 -->
	<sql name="page_queryXhBlackListList" common="false">
		<sqlString>
			<![CDATA[
			$ibpm_queryPageBefore
			$ibpm_queryXhBlackListList
			$ibpm_queryPageAfter
			]]>
		</sqlString>
	</sql>
</sqlmaps>
