<?xml version="1.0" encoding="UTF-8"?>
<sqlmaps>
    <!--  -->
	 <sql name="queryXhMaintainLogList" common="true">
		<sqlString><![CDATA[
			select 
				a.TITLE,/*标题*/
				a.DET_CONTENT,/*详细内容*/
			a.ID,a.CREATE_BY,a.CREATE_TIME
			 from XH_MAINTAIN_LOG a
			      where 1=1
			$sql
			]]></sqlString>
	</sql>
	<!-- 查询 总数 -->
	<sql name="cnt_queryXhMaintainLogList" common="false">
		<sqlString>
			<![CDATA[
			SELECT COUNT(*) RecordCount FROM (
			$ibpm_queryXhMaintainLogList
			)a
		]]>
		</sqlString>
	</sql>
	<!-- 查询 列表分页 -->
	<sql name="page_queryXhMaintainLogList" common="false">
		<sqlString>
			<![CDATA[
			$ibpm_queryPageBefore
			$ibpm_queryXhMaintainLogList
			$ibpm_queryPageAfter
			]]>
		</sqlString>
	</sql>
</sqlmaps>
